{
  "name": "subtitle_game",
  "version": "6.0.0",
  "bin": "server.js",
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "description": "subtitle game",
  "homepage": "https://github.com/valentinMachado/subtitle_game#readme",
  "bugs": {
    "url": "https://github.com/valentinMachado/subtitle_game/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valentinMachado/subtitle_game.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "install-test": "sudo apt-get install ffmpeg && sudo apt get git && sudo ffpmeg && sudo apt install nginx -y && sudo apt update",
    "pm2": "pm2 start npm --name \"subtitle_game\" -- run deploy && pm2 save",
    "deploy": "node ./generate-secret.js && node server.js --binaries=false --wsUrl=https://mache.lol",
    "start": "node server.js --binaries=false",
    "build-window": "node ./copy-socket-client.js  && npx pkg server.js --targets node18-win-x64 --output ./subtitle_game.exe"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "socket.io": "^4.8.3"
  }
}
